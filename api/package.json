{
  "name": "soundchain-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d && yarn migrate && nodemon -w 'src/**' -e 'ts,json' -x ts-node src/local.ts",
    "dev:quick": "yarn migrate && nodemon -w 'src/**' -e 'ts,json' -x ts-node src/local.ts",
    "lint-staged": "lint-staged",
    "seed": "ts-node src/db/seeds.ts",
    "seedProof": "ts-node src/db/seedsProofBook.ts",
    "seed:whitelist": "ts-node src/db/seedWhitelistCsv.ts",
    "seed:audiusholders": "ts-node src/db/seedAudiusHoldersCsv.ts",
    "migrate:create": "sh -c 'ts-node node_modules/migrate-mongo/bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/migrate-mongo/bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:down": "ts-node node_modules/migrate-mongo/bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/migrate-mongo/bin/migrate-mongo up -f migrate-mongo-config.ts",
    "generate-types": "typechain --target=web3-v1 'src/contract/*.json'",
    "build": "tsc"
  },
  "lint-staged": {
    "*.{ts,json,md}": "prettier --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.9",
    "@anatine/esbuild-decorators": "^0.2.19",
    "@magic-sdk/admin": "^1.3.4",
    "@mailchimp/mailchimp_marketing": "^3.0.78",
    "@mailchimp/mailchimp_transactional": "^1.0.59",
    "@mux/mux-node": "^3.3.1",
    "@pinata/sdk": "^1.1.23",
    "@sendgrid/mail": "^7.4.5",
    "@sentry/serverless": "^7.10.0",
    "@typegoose/typegoose": "^7.6.3",
    "@types/aws-lambda": "^8.10.145",
    "abi-decoder": "^2.4.0",
    "apollo-server-express": "^3.0.2",
    "apollo-server-lambda": "^3.0.2",
    "axios": "^0.21.4",
    "base64-url": "^2.3.3",
    "bcryptjs": "^2.4.3",
    "bson": "^4.4.1",
    "cheerio": "^1.0.0-rc.10",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "dot-object": "^2.1.4",
    "dotenv-flow": "^3.2.0",
    "esbuild": "~0.14.29",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.0.0",
    "grapheme-splitter": "^1.0.4",
    "graphql": "^15.5.1",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^8.2.3",
    "mongoose": "5.10.18",
    "patch-package": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-webpack": "^5.14.1",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2",
    "web3": "^1.6.1",
    "typescript": "^4.5.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.25.0",
    "@aws-sdk/s3-request-presigner": "^3.25.0",
    "@typechain/web3-v1": "^4.0.0",
    "@types/base64-url": "^2.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dot-object": "^2.1.2",
    "@types/dotenv-flow": "^3.1.1",
    "@types/express-jwt": "^6.0.4",
    "@types/faker": "^5.5.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/mailchimp__mailchimp_marketing": "^3.0.6",
    "@types/mailchimp__mailchimp_transactional": "^1.0.10",
    "@types/migrate-mongo": "^8.1.3",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "5.10.5",
    "@types/node": "^16.9.6",
    "@types/uuid": "^8.3.4",
    "aws-sdk": "^2.995.0",
    "csvtojson": "^2.0.10",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "serverless-offline": "^14.3.2",
    "typechain": "^6.0.3",
    "webpack": "^5.94.0"
  }
}
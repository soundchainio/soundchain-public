{
  "name": "soundchain-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "build": "next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.{ts,tsx,json,md,css}": "prettier --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "graphql": "^15.5.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-operations": "^1.18.4",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@types/react": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "typescript": "4.3.5"
  }
}
